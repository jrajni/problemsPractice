#include<iostream>
using namespace std;
int main(void)
{
    int n,r,q,reverse=0;
    cin>>n;
    int m=n;
    while(n!=0)
    {
        r=n%10;
        reverse=reverse*10+r;
        n=n/10;
    }
    cout<<reverse;
    cout<<endl<<m<<endl;
    if(reverse!=m)
    {cout<<"NUmber is not Palidrome";}
    else{ cout<<"Number is Palindrome";}
        return 0;
}
